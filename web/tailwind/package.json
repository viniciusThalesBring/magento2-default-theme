{
  "name": "tailwind",
  "version": "1.0.0",
  "description": "Tailwind CSS compiler",
  "main": "tailwind.config.js",
  "dependencies": {
    "csso-cli": "^3.0.0",
    "tailwindcss": "^1.4.6",
    "terser": "^4.7.0"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tailwindcss build tailwind-source.css -o tailwind-output.css",
      "minify": "npx csso tailwind-output.css --output ../css/styles.css",
      "build-dev": "NODE_ENV=development npm run build && npm run minify",
      "build-prod": "NODE_ENV=production npm run build && npm run minify"
  },
